{
  "manifest_version": 2,
  "name": "Open Bookmark Manager",
  "description": "It is an extension for opening bookmark manager.",
  "version": "1.0",
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "48": "images/icon/icon_48.png"
    },
    "default_title": "Open Bookmark Manager"
  }
}